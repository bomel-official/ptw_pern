  "type": "commonjs",
  "scripts": {
    "build": "tsc --outDir dist",
    "dev": "concurrently --kill-others \"npm run dev:tsc\" \"npm run dev:nodemon\"",
    "dev:tsc": "tsc --watch",
    "dev:nodemon": "nodemon -p tsconfig.json ./src/index.ts",
    "dev:ts-node": "npx ts-node -p tsconfig.json -r tsconfig-paths/register src/index.ts",
    "prod": "node dist/index.js",
    "lint": "npx eslint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "jsonwebtoken": "^9.0.0",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5",
    "sharp": "^0.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/express-handlebars": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "globals": "^15.3.0",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "_moduleAliases": {
    "@constants": "core/constants",
    "@controllers": "controllers",
    "@core": "core",
    "@db": "db",
    "@error": "error",
    "@libs": "core/libs",
    "@middleware": "middleware",
    "@types": "core/types"
  }
